<!DOCTYPE html>
<html>
	<head>
		<title>UofT Web Development Club</title>

		<%= render 'layouts/metatags' %>

		<%= csrf_meta_tags %>
		<%= render 'layouts/cssHook' %>
		<%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
		
		<%= yield :head %> 
	</head>
	<body<% if content_for?(:body_attributes) %><%= yield(:body_attributes) %><% end %>>
	<div class="wrapper">
	<div class="topwrap">
		<%= render 'layouts/topModule' %>
		<%= render 'layouts/secondModule' %>
	</div>
		<div class="fixedWrapper">
			<div class="panelModule">
				<%= render 'layouts/panelModule' %>		
			</div>
			<div class="content">
				<%= yield %>
			</div>
		</div>
	</div>
	<div class="footer">
		<%= render 'layouts/footer' %>
	</div>

	</body>
</html>
