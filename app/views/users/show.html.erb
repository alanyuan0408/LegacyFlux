    <script>

      function initialize(){
        //Initialize the object defined in Your Posts
        if (typeof pageObject == "undefined"){
          pageObject = new pageIndex(<%= @feedbank_posts.length %>, "jobPosts", "pagenumber");   
        } 

        pageObject.initialize('.a_userposts', 'posts');
      }

      $(document).ready(initialize);
      $(document).on('page:load', initialize);

    </script>

    <%= render :partial => "users/userPartial/userFeedbankPosts" %>