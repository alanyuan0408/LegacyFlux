
<div class="row">	

	<div class="col-xs-4">
		<div class="row" style="height:20px"></div>

		<div class="text-center">
			<div class="btn-group">
  			<button type="button" class="btn btn-default" 
  			onclick="pageObject3.decrement()">Prev</button>

 			<button type="button" class="btn btn-default disabled" id="pagenumber3">1</button>

  			<button type="button" class="btn btn-default" 
  			onclick="pageObject3.increment()">Next</button>
			</div>
		</div>

		<div class="row" style="height:5px"></div>


		<ul id="itemPosts2" class="nav nav-tabs nav-stacked">
	        <% @unconfirmed_posts.each do |post| %>
	            <li class="list_li"><a data-id="<%= post.item_id %>" class="a_admin"><%= post.item_title %></a></li>
	        <% end %>
	    </ul>

	</div>

	<div class="col-xs-8">
		<div class="row" style="height:20px"></div>
		<div class="panel-content" id= "panel_content" >
			
			<h4 id="postTitle">Title of the Post</h4>
			<hr>
			<div class="row">
				<div class='col-xs-4'>
				</div>
				<div class='col-xs-8'>
					<p id="postURL">Post URL</p>
					<p id="postDate">Post Date</p>
					<hr>
					<p id="postText">Text:</p>
				</div>
			</div>

		</div>
	</div>
</div>